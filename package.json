{
  "name": "incubrix-cms-backend",
  "version": "2.0.0",
  "description": "Incubrix CMS Backend - Upload & Asset Management System with Analytics, File Access & RSS Feed Support",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "reset": "node scripts/reset.js",
    "setup": "npm run migrate && npm run seed",
    "generate-rss": "node scripts/generate-rss.js",
    "validate-rss": "node scripts/validate-rss.js"
  },
  "keywords": [
    "cms",
    "asset-management",
    "file-upload",
    "incubrix",
    "excel-export",
    "analytics",
    "folder-upload",
    "file-access",
    "rss-feed",
    "xml-syndication",
    "content-syndication",
    "rss-2.0",
    "media-enclosure"
  ],
  "author": "Incubrix Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "sqlite3": "^5.1.6",
    "sqlite": "^5.0.1",
    "uuid": "^9.0.1",
    "mime-types": "^2.1.35",
    "ffprobe": "^1.1.2",
    "ffprobe-static": "^3.1.0",
    "pdf-parse": "^1.1.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "xml2js": "^0.6.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "rss": {
    "specification": "RSS 2.0",
    "encoding": "UTF-8",
    "endpoints": [
      "/api/rss/feed",
      "/rss",
      "/rss.xml"
    ],
    "features": [
      "automatic-feed-generation",
      "media-enclosures",
      "custom-categories",
      "guid-support",
      "cdata-descriptions",
      "configurable-settings",
      "bulk-content-management"
    ]
  }
}